{
  "name": "cypress-demowebshop",
  "version": "1.0.0",
  "description": "cypress-webshop",
  "main": "index.js",
  "scripts": {
    "registerLoginLogout": "npx cypress run --browser chrome --spec 'cypress/e2e/demoWebShop/registerLoginLogout.cy.js'",
    "search": "npx cypress run --browser chrome --spec 'cypress/e2e/demoWebShop/search.cy.js'",
    "cart": "npx cypress run --browser chrome --spec 'cypress/e2e/demoWebShop/cart.cy.js'",
    "checkout": "npx cypress run --browser chrome --spec 'cypress/e2e/demoWebShop/checkout.cy.js'",
    "generateReport": "npx cypress run --browser chrome --reporter junit --reporter-options 'mochaFile=reports/TEST-[hash].xml'"
  },
  "author": "arrifqiaziz",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.2.0"
  }
}
